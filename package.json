{
    "name": "custom-chatbot",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"next dev --turbopack\" \"npm run start-api\"",
        "start-api": "bash -c 'python3.12 -m venv venv && source venv/bin/activate && pip install -r requirements.txt && uvicorn api.index:app --reload'",
        "build": "next build",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@ai-sdk/google": "^1.0.9",
        "@tabler/icons-react": "^3.27.1",
        "ai": "^4.0.16",
        "clsx": "^2.1.1",
        "markdown-to-jsx": "^7.7.2",
        "next": "15.1.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwind-merge": "^2.6.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "concurrently": "^9.1.2",
        "eslint": "^9",
        "eslint-config-next": "15.1.0",
        "postcss": "^8",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    },
    "prettier": {
        "tabWidth": 4,
        "plugins": [
            "prettier-plugin-tailwindcss"
        ]
    }
}
